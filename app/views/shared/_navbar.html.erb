<div class="navbar navbar-expand-sm navbar-light navbar-lewagon sticky-top">
  <div class="container-fluid">
      <%=  image_tag "https://blog.gumtree.com.au/wp-content/uploads/2017/07/Logo-for-blog.jpg", style: "border-radius:10px;" %>

        <button name="button" type="submit" class="btn btn-flat">
          <i class="fas fa-search"></i> Search
        </button>
      </div>

    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav me-auto">
        <% if user_signed_in? %>
          <li class="nav-item">
            <%= link_to "Messages", "#", class: "nav-link", style: "color: white;" %>
          </li>
          <li class="nav-item">
            <%= link_to "Log out", destroy_user_session_path, data: {turbo_method: :delete}, class: "nav-link", style: "color: white;" %>
          </li>
        <% else %>
          <li class="nav-item">
            <%= link_to "Login/Register", new_user_session_path, class: "nav-link", style: "color: white;" %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
